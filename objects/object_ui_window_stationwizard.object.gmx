        drawShadowedText(textLeft, top+interiorPadding+textInteriorPadding+titleSupposedHeight, 
                        translate("STATION_WIZARD", "welcomeText"),
                        colors,
                        textRight-textLeft);
                        
        var cb = "[_]";
