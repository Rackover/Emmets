        drawBorders(pos, array(COLOR_FOREGROUND(), c_black, COLOR_BACKGROUND()));
        
        var text = mottoContent;
        if (selectedInput == "motto"){
            text += "_";
        }
        
        draw_set_valign(fa_middle); 
